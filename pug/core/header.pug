// header
header#headerSticky.header
  .container
    .header__container
      a(href="#").header__logo 
        img(src="img/logo-colors.svg", alt="")
      nav.header__nav#menuMobile
        ul.header__menu 
          li.active
            a(href="#") О проекте
          li 
            a(href="#") программы
          li 
            a(href="#") услуги
          li 
            a(href="#") новости
          li 
            a(href="#") Контакты
        .header__buttons.d-block.d-md-none 
          button.btn.btn--green-border Войти
          button.btn.btn--green Стать резидентом
        .header__close.js-drop-close(data-drop="menuMobile")
      .header__buttons.d-none.d-md-block
        button.btn.btn--green-border Войти
        button.btn.btn--green Стать резидентом
      button.hamburger.hamburger--squeeze.js-drop-menu(data-drop="menuMobile")
        span.hamburger-box
          span.hamburger-inner
//	/header